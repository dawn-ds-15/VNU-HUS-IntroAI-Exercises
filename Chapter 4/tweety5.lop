% Tweety5: Circumscription style (minimize abnormal)
fof(bird_default, axiom, ![X] : (bird(X) & ~ab(X) => fly(X))).
fof(penguin_ab, axiom, ![X] : (penguin(X) => ab(X))).
fof(penguin_is_bird, axiom, ![X] : (penguin(X) => bird(X))).
fof(tweety_penguin, axiom, penguin(tweety)).
fof(conj_not_fly, conjecture, ~fly(tweety)).
